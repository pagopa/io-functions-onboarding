{
  "name": "io-functions-onboarding",
  "description": "IO onboarding portal API for public administrations",
  "author": "https://pagopa.gov.it",
  "repository": "https://github.com/pagopa/io-functions-onboarding",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "extensions:install": "func extensions install",
    "start": "func start --javascript",
    "predev": "npm-run-all generate build",
    "dev": "npm-run-all --parallel start watch",
    "docker:dev": "docker-compose -f docker-compose.yaml -f docker-compose.override.dev.yaml up --build",
    "preprepare-db": "yarn build",
    "prepare-db": "node src/database/prepare.js",
    "test": "dotenv -e env.example -- jest",
    "test:coverage": "dotenv -e env.example -- jest --coverage",
    "lint": "tslint -p .",
    "lint-api": "npx @redocly/openapi-cli validate openapi/index.yaml",
    "dist:modules": "modclean -r -n default:safe && yarn install --production",
    "dist:zip": "zip -r dist.zip . --exclude @.funcignore --exclude .funcignore",
    "predeploy": "npm-run-all generate build dist:*",
    "generate:api-models": "shx rm -rf src/generated/api && mkdir -p src/generated/api && gen-api-models --api-spec https://raw.githubusercontent.com/pagopa/io-functions-admin/master/openapi/index.yaml --out-dir src/generated/api --no-strict --request-types --response-decoders",
    "generate:onboarding-models": "shx rm -rf src/generated/onboarding && mkdir -p src/generated/onboarding && gen-api-models --api-spec ./openapi/index.yaml --out-dir src/generated/onboarding --no-strict --request-types --response-decoders",
    "generate": "npm-run-all generate:*"
  },
  "devDependencies": {
    "@azure/functions": "^1.0.3",
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.0",
    "@types/event-stream": "^3.3.34",
    "@types/express": "^4.17.2",
    "@types/imap-simple": "^4.2.2",
    "@types/jest": "^24.0.15",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "10.14.1",
    "@types/node-cron": "^2.0.2",
    "@types/node-fetch": "^2.5.0",
    "@types/nodemailer": "^6.2.1",
    "@types/passport": "^1.0.2",
    "@types/passport-http-bearer": "^1.0.33",
    "@types/pdfkit": "^0.10.3",
    "@types/pg": "^7.4.14",
    "@types/tmp": "^0.1.0",
    "@types/validator": "^10.11.1",
    "@types/vfile-message": "^2.0.0",
    "danger": "^8.0.0",
    "danger-plugin-digitalcitizenship": "*",
    "dotenv-cli": "^3.0.0",
    "italia-tslint-rules": "*",
    "jest": "^24.9.0",
    "jest-mock-express": "^0.1.1",
    "mock-fs": "^4.10.3",
    "modclean": "^3.0.0-beta.1",
    "nock": "^11.7.2",
    "nodemailer-mock-transport": "^1.3.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "release-it": "^12.4.3",
    "shx": "^0.3.2",
    "sqlite3": "^4.1.1",
    "swagger-cli": "^2.3.1",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@pagopa/io-functions-express": "^1.0.0",
    "accesscontrol": "^2.2.1",
